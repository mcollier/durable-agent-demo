{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "%TASKHUB_NAME%",
      "storageProvider": {
        "type": "azureManaged",
        "connectionStringName": "DURABLE_TASK_SCHEDULER_CONNECTION_STRING"
      },
      "tracing": {
        "distributedTracingEnabled": true,
        "version": "V2"
      }
    },
    "serviceBus": {
      "prefetchCount": 0,
      "autoCompleteMessages": true
    }
  }
}
